<!--miniprogram/pages/setting/feedback.wxml-->
<form bindsubmit="formSubmit" bindreset="formReset">
  <view class="section">
    <view class="section__title">反馈</view>
    <input name="input" placeholder="输入你的反馈或者问题" />
  </view>
  <view class="btn-area">
    <button form-type="submit">Submit</button>
  </view>
</form>
<view class="weui-toptips weui-toptips_warn" wx:if="{{error}}">{{error}}</view>